<section id="projects">
	<div class="row-1">
		<h2 class="highlight">Projects</h2>
	</div>
	<div class="row-2">
		<div id="all-projects">
			<% projectData.forEach((project, index, array) => { %>
			<div class="project-card">
				<img
					class="project-image"
					src="<%= project.image %>"
					alt="Project Image"
				/>
				<div class="project-title">
					<h2><%= project.title %></h2>
					<div class="project-links">
						<a href="<%= project.web %>" class="project-web">
							<svg
								class="project-web-svg"
								width="800px"
								height="800px"
								viewBox="0 0 16 16"
								fill="none"
								xmlns="http://www.w3.org/2000/svg"
							>
								<path
									d="M7.05025 1.53553C8.03344 0.552348 9.36692 0 10.7574 0C13.6528 0 16 2.34721 16 5.24264C16 6.63308 15.4477 7.96656 14.4645 8.94975L12.4142 11L11 9.58579L13.0503 7.53553C13.6584 6.92742 14 6.10264 14 5.24264C14 3.45178 12.5482 2 10.7574 2C9.89736 2 9.07258 2.34163 8.46447 2.94975L6.41421 5L5 3.58579L7.05025 1.53553Z"
									fill="#C2D9FF"
								/>
								<path
									d="M7.53553 13.0503L9.58579 11L11 12.4142L8.94975 14.4645C7.96656 15.4477 6.63308 16 5.24264 16C2.34721 16 0 13.6528 0 10.7574C0 9.36693 0.552347 8.03344 1.53553 7.05025L3.58579 5L5 6.41421L2.94975 8.46447C2.34163 9.07258 2 9.89736 2 10.7574C2 12.5482 3.45178 14 5.24264 14C6.10264 14 6.92742 13.6584 7.53553 13.0503Z"
									fill="#C2D9FF"
								/>
								<path
									d="M5.70711 11.7071L11.7071 5.70711L10.2929 4.29289L4.29289 10.2929L5.70711 11.7071Z"
									fill="#C2D9FF"
								/>
							</svg>
						</a>
						<a href="<%= project.github %>" class="project-github">
							<svg
								class="project-github-svg"
								xmlns="http://www.w3.org/2000/svg"
								viewBox="0 0 128 128"
							>
								<g fill="#C2D9FF">
									<path
										fill-rule="evenodd"
										clip-rule="evenodd"
										d="M64 5.103c-33.347 0-60.388 27.035-60.388 60.388 0 26.682 17.303 49.317 41.297 57.303 3.017.56 4.125-1.31 4.125-2.905 0-1.44-.056-6.197-.082-11.243-16.8 3.653-20.345-7.125-20.345-7.125-2.747-6.98-6.705-8.836-6.705-8.836-5.48-3.748.413-3.67.413-3.67 6.063.425 9.257 6.223 9.257 6.223 5.386 9.23 14.127 6.562 17.573 5.02.542-3.903 2.107-6.568 3.834-8.076-13.413-1.525-27.514-6.704-27.514-29.843 0-6.593 2.36-11.98 6.223-16.21-.628-1.52-2.695-7.662.584-15.98 0 0 5.07-1.623 16.61 6.19C53.7 35 58.867 34.327 64 34.304c5.13.023 10.3.694 15.127 2.033 11.526-7.813 16.59-6.19 16.59-6.19 3.287 8.317 1.22 14.46.593 15.98 3.872 4.23 6.215 9.617 6.215 16.21 0 23.194-14.127 28.3-27.574 29.796 2.167 1.874 4.097 5.55 4.097 11.183 0 8.08-.07 14.583-.07 16.572 0 1.607 1.088 3.49 4.148 2.897 23.98-7.994 41.263-30.622 41.263-57.294C124.388 32.14 97.35 5.104 64 5.104z"
									/>
									<path
										d="M26.484 91.806c-.133.3-.605.39-1.035.185-.44-.196-.685-.605-.543-.906.13-.31.603-.395 1.04-.188.44.197.69.61.537.91zm2.446 2.729c-.287.267-.85.143-1.232-.28-.396-.42-.47-.983-.177-1.254.298-.266.844-.14 1.24.28.394.426.472.984.17 1.255zM31.312 98.012c-.37.258-.976.017-1.35-.52-.37-.538-.37-1.183.01-1.44.373-.258.97-.025 1.35.507.368.545.368 1.19-.01 1.452zm3.261 3.361c-.33.365-1.036.267-1.552-.23-.527-.487-.674-1.18-.343-1.544.336-.366 1.045-.264 1.564.23.527.486.686 1.18.333 1.543zm4.5 1.951c-.147.473-.825.688-1.51.486-.683-.207-1.13-.76-.99-1.238.14-.477.823-.7 1.512-.485.683.206 1.13.756.988 1.237zm4.943.361c.017.498-.563.91-1.28.92-.723.017-1.308-.387-1.315-.877 0-.503.568-.91 1.29-.924.717-.013 1.306.387 1.306.88zm4.598-.782c.086.485-.413.984-1.126 1.117-.7.13-1.35-.172-1.44-.653-.086-.498.422-.997 1.122-1.126.714-.123 1.354.17 1.444.663zm0 0"
									/>
								</g>
							</svg>
						</a>
					</div>
				</div>
				<p><%= project.description %></p>
				<% for (let i = 0; i < project.technologies.length && i < 4;
				i++) { %> <% let tech = project.technologies[i]; %>
				<div class="techs techs-icon-box">
					<%= tech.name %>
					<img
						class="tech-svg"
						src="/img/tech/<%= tech.name %>.svg"
						alt="<%= tech.name %>"
					/>
				</div>
				<% } %> <% if (project.technologies.length > 4) { %>
				<div class="techs more-techs pr-mt-<%=index%>">
					<%= project.technologies.length - 4 %> more...
					<div class="more-techs-tooltip pr-mt-<%=index%>">
						<% for (let j = 4; j < project.technologies.length; j++)
						{ %> <% let tech = project.technologies[j]; %>
						<div class="techs techs-icon-box">
							<%= tech.name %>
							<img
								class="tech-svg"
								src="/img/tech/<%= tech.name %>.svg"
								alt="<%= tech.name %>"
							/>
						</div>
						<% } %>
					</div>
				</div>
				<% } %>
			</div>
			<% }); %>
		</div>
	</div>
	<div class="row-3"></div>
</section>
